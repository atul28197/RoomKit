<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ApplicationIcon>roomrowcirc_buF_icon.ico</ApplicationIcon>
    <Version>0.8.1</Version>
    <Authors>Anthony A. Hauck</Authors>
    <Company>Hypar Inc.</Company>
    <Product></Product>
    <Copyright>Hypar Inc.</Copyright>
    <Description>RoomKit provides functions for creating and placing architectural rooms, corridors, services, stories, and towers.</Description>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <AssemblyVersion>0.0.8.1</AssemblyVersion>
    <FileVersion>0.0.8.1</FileVersion>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageTags>AEC Hypar Generative Design Buildings Architecture</PackageTags>
    <PackageProjectUrl>https://github.com/hypar-io/RoomKit</PackageProjectUrl>
    <RepositoryUrl>https://github.com/hypar-io/RoomKit</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryType>C#</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="nuget\**" />
    <EmbeddedResource Remove="nuget\**" />
    <None Remove="nuget\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="clipper.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="RoomKit.0.7.8.nupkg" />
    <None Remove="RoomKit.nuspec" />
    <None Remove="roomrowcirc_buF_icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="GeometryEx" Version="1.2.0" />
    <PackageReference Include="Hypar.Elements" Version="0.2.4.4" />
    <PackageReference Include="NuGet.CommandLine" Version="4.9.2" />
    <PackageReference Include="ParagonClipper" Version="6.4.2" />
  </ItemGroup>

</Project>
